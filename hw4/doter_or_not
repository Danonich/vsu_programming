from collections import deque


def potomushto_ti_doter(s):
    return not len(s) % 2 and s[0] == 'D'


def moshet_ti_doter(deq, people_dict):
    checked_people = []
    while deq:
        chel = deq.popleft()
        if chel not in checked_people:
            if potomushto_ti_doter(chel):
                return chel
            else:
                deq += people_dict.get(chel, [])
            checked_people.append(chel)
    return 'Nobody players in Dota'


people = {
    'Alice': ['Vova', 'Vlados', 'Diana'],
    'Vlados': ['Lila', 'Ahmed'],
    'Diana': ['Daniel'],
    'Vova': ['Vlados', 'Oleja']
}

d = deque(people['Alice'])

print(moshet_ti_doter(d, people))
